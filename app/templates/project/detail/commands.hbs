{{#unless help}}
    <div class="ui active inverted dimmer absolute-fill">
        <div class="ui text loader">
            Loading project ember commands <br>
        </div>
    </div>
{{else}}
    {{#each addons as |addon|}}
        <h5 class="ui centered top attached header">
          <code>{{addon.name}}</code> commands
        </h5>
        {{#each addon.commands key='name' as |cmd|}}
          {{ember-cmd project=model cmd=cmd}}
        {{/each}}
    {{/each}}

    <h5 class="ui centered top attached header">default commands</h5>
    {{#each availableCommands key='name' as |cmd|}}
      {{ember-cmd project=model cmd=cmd}}
    {{/each}}

    <br>

{{/unless}}
{{outlet}}
{{outlet}}

<div class="ui basic segment">
    <div class="ui segment">
        <h4 class="ui header">Project settings</h4>

        <div class="ui divider"></div>

        <form class="ui form" {{action 'saveChanges' on='submit'}}>

            <div class="equal width fields">
                <div class="field">
                    <div class="ui checkbox">
                      {{input
                      type="checkbox"
                      class="hidden"
                      name="disableAnalytics"
                      id="settings-disableAnalytics"
                      checked=disableAnalytics}}
                        <label for="settings-disableAnalytics">disableAnalytics</label>
                    </div>
                </div>
                <div class="field">
                    <div class="ui checkbox">
                      {{input
                      type="checkbox"
                      class="hidden"
                      name="liveReload"
                      id="settings-liveReload"
                      checked=liveReload}}
                        <label for="settings-liveReload">liveReload</label>
                    </div>
                </div>
                <div class="field">
                    <div class="ui checkbox">
                      {{input
                      type="checkbox"
                      class="hidden"
                      name="checkForUpdates"
                      checked=checkForUpdates
                      id="settings-checkForUpdates"}}
                        <label for="settings-checkForUpdates">checkForUpdates</label>
                    </div>
                </div>
            </div>

            <div class="field">
                <div class="fields">
                    <div class="eight wide field">
                        <label>Port</label>
                      {{input
                      type="number"
                      min="0"
                      step="1"
                      name="port"
                      placeholder="4200"
                      value=port}}
                    </div>
                    <div class="eight wide field">
                        <label>Test port</label>
                      {{input
                      type="number"
                      min="0"
                      step="1"
                      name="testPort"
                      placeholder="7357"
                      value=testPort}}
                    </div>
                </div>
            </div>

